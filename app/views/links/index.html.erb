

<div class="container">
  <% @links.each do |link| %>
  <div class="link row clearfix">
    <div class="text-center">
      <h2>
       <%= link_to link.title, link %><br>
       <h4><small class="author">Submitted <%= time_ago_in_words(link.created_at) %> ago by <%= link.user.email %></small></h4>
     </h2>
     <hr class="col-md-offset-3 col-md-6">
   </div>
 </div>
 <% end %>
</div>















